package jspiders;

public class patterns {
	public static void main(String[] args) {
		int n = 6;
		char c = 'A';
		for(int i = 1 ; i<=6;i++) {
			for (int j=1;j<=6;j++) {
				if(i+j>=n+1) {
					System.out.print(c+" ");
					c++;
				}
				else {
					System.out.print(" ");
				}
			}
		System.out.println();
		}
		
	}
}
