package jspiders;

public class patterns {
	public static void main(String[] args) {
		int n = 3;
		int c = 1;
		for(int i = 1 ; i<=3;i++) {
			for (int j=1;j<=3;j++) {
				if(i+j>=n+1) {
					System.out.print(c+" ");
					c++;
				}
				else {
					System.out.print("");
				}
			}
		System.out.println();
		}
		
	}
}
