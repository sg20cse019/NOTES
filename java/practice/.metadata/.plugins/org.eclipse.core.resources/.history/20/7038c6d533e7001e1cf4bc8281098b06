package javabasic;
public class javabasic1{
    public static void main(String[] args) {
        int startingbal = 500;
        int endingbal = 0;
        int itemprice = {12,400,32,34,87,90};
        int i = 0;
        System.out.println(startingbal);
        System.out.println(endingbal);

        while (startingbal >= endingbal) {
            startingbal -= itemprice[i;
            i++;
            System.out.println(startingbal);
        }

        System.out.println("Insufficient balance.");
        System.out.println("Total number of successful withdrawals: " + i);
    }
}
