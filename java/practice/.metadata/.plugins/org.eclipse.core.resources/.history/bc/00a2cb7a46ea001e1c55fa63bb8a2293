package javabasic;
import java.util.*;

public class javabasic1{
	public static void main(String[] args) {
		System.out.println("enter the two numbers");
		Scanner sc= new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int sum = a+b;
		System.out.println("total sum is "+ sum);
	}
}