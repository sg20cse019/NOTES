package javabasic;

public class javabasic1 {
 
    public static void main(String[] args) {
    	int bal = 175000;
        int withdraw = 2000;
        int i=1;
       while(i<=90) {
    	   if (bal==0) {
    		   System.out.println(i);
    	   }
    	   System.out.println(bal -= withdraw);
    	  i++;
      }
    }
}