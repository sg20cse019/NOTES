package javabasic;
import java.util.*;

public class javabasic1{
	public static void main(String[] args) {
		System.out.println("enter the number ");
		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt();
		for (int i=1;i<=n;i++) {
			n = i*(n*n-i);
			System.out.println(n);
		}
	}
}