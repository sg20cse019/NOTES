package javabasic;

public class javabasic1 {
 
    public static void main(String[] args) {
    	int bal = 175000;
        int withdraw = 2000;
      
      for(int i=1 ; i<=100 ; i++) {

      System.out.println(bal -= withdraw);
      if(bal==0) {
		  System.out.println("no of trancations "+ i);
		  break;
	  }
    	  
      }
    }
}