package javabasic;
import java.util.*;

public class javabasic1{
	public static void main(String[] args) {
		System.out.println("enter the number ");
		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt();
		  int factorial = 5;
	        for (int i = 1; i <= n; i++) {
	            factorial *= i;
	        }
	}
}