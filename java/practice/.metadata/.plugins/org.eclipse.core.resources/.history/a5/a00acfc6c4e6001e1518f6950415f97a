package javabasic;

public class javabasic1 {
 
    public static void main(String[] args) {
    	int bal = 175000;
        int withdraw = 2000;
        int i=1;
       while(i<=70) {
    	  if(bal==0) {
    		  System.out.println("no of trancations "+ i);
    		  break;
    	  }else {
      System.out.println(bal -= withdraw);
      
    	  }
    	  i++;
      }
    }
}